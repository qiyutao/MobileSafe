<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seven.mobilesafe">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.SplashActivity"
            android:icon="@drawable/icon5">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MainActivity"
            android:label="主界面"
            android:theme="@android:style/Theme.NoTitleBar">

        </activity>

        <activity android:name=".ui.LostProtectActivity"
            android:label="手机防盗"
            />
        <activity android:name=".ui.SetupGuideActivity"
            android:label="@string/setup"
            />
        <activity android:name=".ui.SetupGuide2Activity"
            android:label="setup2"
            />
        <activity android:name=".ui.SetupGuide3Activity"
            android:label="setup3"
            />
        <activity android:name=".ui.SetupGuide4Activity"
            android:label="setup4"
            />


    </application>

    

</manifest>